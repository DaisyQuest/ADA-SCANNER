@{
    var title = "GoldMaster Case";
}
<!doctype html>
<html lang="en">
<head>
<meta charset="utf-8">
<title>@title</title>
</head>
<body>
<!-- Scenario: Pointer gestures, motion actuation, and character key shortcuts (InteractionLimitsCheck) -->
<div id="content">
<main id="main">
<h1>@title</h1>
<button accesskey="s" onpointerdown="handlePointer()">Shortcut Action</button>
<div ontouchstart="handleTouch()" role="button">Touch Gesture</div>
<script>
window.addEventListener("deviceorientation", () => {
  console.log("motion event");
});
</script>
</main>
</div>
</body>
</html>
